<gora-otd xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
          xsi:noNamespaceSchemaLocation="gora-neo4j.xsd">
  <class name="org.apache.gora.examples.generated.Employee" keyClass="java.lang.String" label="Employee">
    <nodeKey property="pkssn" type="VARCHAR" />
    <field name="ssn" property="ssn" type="VARCHAR" exists="true" />
    <field name="value" property="value" type="VARCHAR" exists="true" />
    <field name="name" property="name" type="VARCHAR" exists="true" />
    <field name="dateOfBirth" property="dateOfBirth" type="INTEGER" exists="true" />
    <field name="salary" property="salary" type="INTEGER" exists="true" />
    <field name="boss" property="boss" type="JAVA_OBJECT" exists="true" />
    <field name="webpage" property="webpage" type="JAVA_OBJECT" exists="false" />
  </class>
  
  <class name="org.apache.gora.examples.generated.WebPage" keyClass="java.lang.String" label="Webpage">
    <nodeKey property="pkurl" type="VARCHAR" />
    <field name="url" property="url" type="VARCHAR" exists="true" />
    <field name="content" property="content" type="VARCHAR" exists="true" />
    <field name="parsedContent" property="parsedContent" type="ARRAY" exists="true" />
    <field name="outlinks" property="outlinks" type="JAVA_OBJECT" exists="true" />
    <field name="headers" property="headers" type="JAVA_OBJECT" exists="true" />
    <field name="metadata" property="metadata" type="VARCHAR" exists="true" />
    <field name="byteData" property="byteData" type="JAVA_OBJECT" exists="true" />
    <field name="stringData" property="stringData" type="JAVA_OBJECT" exists="true" />
  </class>

</gora-otd>
